{
  "name": "vs-slack",
  "displayName": "vs-slack",
  "publisher": "CalebDenio",
  "description": "Slack integration for VS Code that's actually useful.",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjdenio/vs-slack"
  },
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vs-slack.openSlack",
    "onCommand:vs-slack.login"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-slack.openSlack",
        "title": "vs-slack: Open Slack"
      },
      {
        "command": "vs-slack.login",
        "title": "vs-slack: Log in to Slack"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile && yarn run ui:compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "ui:watch": "webpack --watch --mode=development",
    "ui:compile": "webpack --mode=production"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/markdown-it": "^12.0.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.3",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "css-loader": "^5.2.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@slack/rtm-api": "^6.0.0",
    "@slack/web-api": "^6.1.0",
    "axios": "^0.21.1",
    "markdown-it": "^12.0.4",
    "markdown-it-slack": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
